<div class="container">
  <div class="row">
    <div class="col">
        <% if (profileObject.image) { %>
        <img src="<%= profileObject.image %>" />
        <% } else { %>

        <img src="https://i.pravatar.cc/300" />
        <% } %>
   
      <h1><%= profileObject.firstname %> <%= profileObject.lastname %></h1>
    </div>
  </div>
</div>
